{
  "editor.fontSize": 18,
  "editor.minimap.scale": 3,
  "editor.glyphMargin": false,
  "editor.mouseWheelZoom": true,
  "vs-kubernetes": {
    "vs-kubernetes.helm-path.linux": "/home/pachinko/.vs-kubernetes/tools/helm/linux-amd64/helm",
    "vs-kubernetes.draft-path.linux": "/home/pachinko/.vs-kubernetes/tools/draft/linux-amd64/draft",
